        }], "@babel/preset-react"
    ],
    "plugins": ["react-hot-loader/babel", "@babel/plugin-proposal-export-namespace-from", "@babel/plugin-proposal-class-properties", "@babel/plugin-transform-runtime", "@babel/plugin-syntax-dynamic-import"]
}
